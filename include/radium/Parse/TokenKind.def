#ifndef RADIUM_KEYWORD
#define RADIUM_KEYWORD(Name)
#endif

RADIUM_KEYWORD(void)
RADIUM_KEYWORD(int)
RADIUM_KEYWORD(var)

#ifndef RADIUM_PUNCTUATOR
#define RADIUM_PUNCTUATOR(X, Y)
#endif

RADIUM_PUNCTUATOR(LParen,  "(")
RADIUM_PUNCTUATOR(RParen,  ")")
RADIUM_PUNCTUATOR(LBrace,  "{")
RADIUM_PUNCTUATOR(RBrace,  "}")
RADIUM_PUNCTUATOR(LSquare, "[")
RADIUM_PUNCTUATOR(RSquare, ']')

RADIUM_PUNCTUATOR(Comma,   ",")
RADIUM_PUNCTUATOR(Colon,   ":")
RADIUM_PUNCTUATOR(Semi,    ";")
RADIUM_PUNCTUATOR(Equal,   "=")

RADIUM_PUNCTUATOR(Plus,    "+")
RADIUM_PUNCTUATOR(Minus,   "-")
RADIUM_PUNCTUATOR(Star,    "*")
RADIUM_PUNCTUATOR(Slash,   "/")

RADIUM_PUNCTUATOR(Arrow,   "->")
