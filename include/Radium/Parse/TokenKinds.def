#ifndef RADIUM_KEYWORD
#define RADIUM_KEYWORD(Name)
#endif

// Keywords that start decls.
RADIUM_KEYWORD(class)
RADIUM_KEYWORD(destructor)
RADIUM_KEYWORD(enum)
RADIUM_KEYWORD(extension)
RADIUM_KEYWORD(func)
RADIUM_KEYWORD(import)
RADIUM_KEYWORD(init)
RADIUM_KEYWORD(let)
RADIUM_KEYWORD(protocol)
RADIUM_KEYWORD(static)
RADIUM_KEYWORD(struct)
RADIUM_KEYWORD(subscript)
RADIUM_KEYWORD(typealias)
RADIUM_KEYWORD(var)

// Statement keywords.
RADIUM_KEYWORD(if)
RADIUM_KEYWORD(do)
RADIUM_KEYWORD(else)
RADIUM_KEYWORD(for)
RADIUM_KEYWORD(in)
RADIUM_KEYWORD(while)
RADIUM_KEYWORD(return)
RADIUM_KEYWORD(break)
RADIUM_KEYWORD(continue)
RADIUM_KEYWORD(fallthrough)
RADIUM_KEYWORD(switch)
RADIUM_KEYWORD(case)
RADIUM_KEYWORD(default)
RADIUM_KEYWORD(where)

RADIUM_KEYWORD(undef)       // This is only enabled when parsing .ril files.
RADIUM_KEYWORD(ril)         // This is only enabled when parsing .ril files.
RADIUM_KEYWORD(ril_stage)   // This is only enabled when parsing .ril files.
RADIUM_KEYWORD(ril_vtable)  // This is only enabled when parsing .ril files.
RADIUM_KEYWORD(ril_global)  // This is only enabled when parsing .ril files.
RADIUM_KEYWORD(
    ril_witness_table)  // This is only enabled when parsing .ril files.

